        <table class="cgs-dashboard-gandeffort-table">
        <thead> 
            <tr class="text-center">
                <th colspan=""></th>
                {{#years_grades}}
                {{#labels}}
                  <th colspan="4">{{label}} </th>               
                {{/labels}}
                {{/years_grades}}
            </tr>
    
        </thead>
        <tbody>
            <tr class="text-center">    
                <th>{{#str}} classlabel, report_mystudent {{/str}}</th>

                {{#years_grades}}
                {{#labels}}
                    {{#termlabels}}   
                        <th>{{t1}}</th>
                        <th>{{t2}}</th>
                        <th>{{t3}}</th>  
                        <th>{{t4}}</th>  
                    {{/termlabels}}
                {{/labels}}
                {{/years_grades}}
            </tr>

            
            {{#subjectdetails_grades}}
            {{#classes}}
                <tr>
                    <td>{{name}}</td>
                    {{#grades}} 
                        {{#grade}}
                        <td style="text-align: center; background-color: {{bcolour}}; color: {{fcolour}}">{{grade}}</td>
                        {{/grade}}
                    {{/grades}}
                </tr>
            {{/classes}}
            {{/subjectdetails_grades}}
       
        </tbody>
    </table>
