<table id = 'grade-history-table' class="cgs-dashboard-gandeffort-table table-wrapper-scroll-y my-custom-scrollbar grade-history-table grade-history-table-senior">
        <thead> 
            <tr class="text-center reports-heading">
                <th style="width: 100%;">{{#str}} classlabel, report_mystudent {{/str}}</th>
                {{#years_grades}}
                {{#labels}}
                  <th colspan="4">{{label}} </th>               
                {{/labels}}
                {{/years_grades}}
            </tr>
        </thead>
        <tbody>
            <tr class="text-center reports-heading">    
                <td></td>
                {{#years_grades}}
                {{#labels}}
                    {{#termlabels}}   
                        <td>{{t1}}</td>
                        <td>{{t2}}</td>
                        <td>{{t3}}</td>  
                        <td>{{t4}}</td>  
                    {{/termlabels}}
                {{/labels}}
                {{/years_grades}}
            </tr>
            
            {{#subjectdetails_grades}}
            {{#classes}}
                <tr>
                    <td>{{name}}</td>
                    {{#grades}} 
                        {{#grade}}
                        <td style="text-align: center; background-color: {{bcolour}}; color: {{fcolour}}">{{grade}}</td>
                        {{/grade}}
                    {{/grades}}
                </tr>
            {{/classes}}
            {{/subjectdetails_grades}}
       
        </tbody>
</table>
