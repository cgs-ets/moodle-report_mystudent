    <table id = "effort-history-table" class="cgs-dashboard-gandeffort-table table-wrapper-scroll-y my-custom-scrollbar effort-history-table">
        <thead>
            <tr class="text-center">
                <th style="width: 100%;">{{#str}} classlabel, report_mystudent {{/str}}</th>
                {{#years_effort}}
                {{#labels}}
                <th colspan="4">{{label}} </th>
                {{/labels}}
                {{/years_effort}}
            </tr>

        </thead>
        <tbody>
            <tr class="text-center">
                <th></th>
                {{#years_effort}}
                {{#labels}}
                {{#termlabels}}
                <th>{{t1}}</th>
                <th>{{t2}}</th>
                <th>{{t3}}</th>
                <th>{{t4}}</th>
                {{/termlabels}}
                {{/labels}}
                {{/years_effort}}
            </tr>


            {{#subjectdetails_effort}}
            {{#classes}}
            <tr>
                <td>{{name}}</td>
                {{#grades}}
                {{#grade}}
                <td data-html="true" data-toggle="tooltip" data-placement="bottom" title="{{{notes}}}"
                    style="text-align: center; background-color: {{bcolour}}; ">
                    {{grade}}
                </td>
                {{/grade}}
                {{/grades}}
            </tr>
            {{/classes}}
            {{/subjectdetails_effort}}

        </tbody>
    </table>
